<% content_for :navigation do %>
  <ul>
  <li><a href="/products"><img src="/products.png"></a></li>
  <li><a href="/blog"><img src="/blog.png"></a></li>
  </ul>
<% end %>
<% content_for :page do %>
  <h1>Archive</h1>
  <table class="archive">
  <% posts.each do |post| %>
  	<tr>
  		<td class="date"><%= post.created_at.strftime("%B %d, %Y") %></td>
  		<td class="title"><a href="<%= post.url %>"><%= post.title %></a></td>
  	</tr>
  <% end %>
  </table>
<% end %>